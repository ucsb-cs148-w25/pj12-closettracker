{
  "expo": {
    "name": "Closet Tracker",
    "slug": "Closet-Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/1024x1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontend",
      "icon": "./assets/icons/1024x1024.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/512x512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/round512.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "android": {
            "resizeMode": "contain",
            "backgroundColor": "#ffffff",
            "image": "./assets/images/splash-1.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
